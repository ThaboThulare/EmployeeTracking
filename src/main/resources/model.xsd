<schema version="1.0"
        xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://employeetracking.co.za/model"
        xmlns:tns="http://employeetracking.co.za/model"
        elementFormDefault="qualified">

    <element name="UserDetailQueryModel" type="tns:UserDetailQueryModel"/>
    <complexType name="UserDetailQueryModel">
        <sequence>
            <element name="id" type="long"></element>
            <element name="name" type="string"></element>
            <element name="lastName" type="string"></element>
            <element name="idNumber" type="string"></element>
        </sequence>
    </complexType>

    <element name="ScannerQueryModel">
        <complexType>
            <sequence>
                <element name="id" type="long"></element>
                <element name="loginTime" type="string"></element>
                <element name="logoutTime" type="string"></element>
                <element name="userDetails" minOccurs="0" maxOccurs="1" type="tns:UserDetailQueryModel"></element>
            </sequence>
        </complexType>
    </element>


</schema>
